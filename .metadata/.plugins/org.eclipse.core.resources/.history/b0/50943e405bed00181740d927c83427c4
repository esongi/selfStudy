package javaStudy.method;

public class ObjectMethod {

	public static void main(String[] args) {
		Object intObj = 1234;
		Integer intInteger = 1234;
		int intTemp = 1234;

		System.out.println("값은 :" + intObj.equals(intInteger));
		System.out.println("본질은: " + (intObj == intInteger));
		
		System.out.println("값은 :" + intInteger.equals(intTemp));
		System.out.println("본질은: " + (intInteger == intTemp));
		
	}

}
